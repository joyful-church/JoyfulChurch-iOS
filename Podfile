# platform :ios, '9.0'

target 'JoyfulChurch' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  #ReactiveX
  pod 'RxSwift'
  pod 'RxCocoa'

  #DataBase
  pod 'Realm'

  pod 'Alamofire'
  pod 'Kingfisher'

  #Firebase
  pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/RemoteConfig'
  pod 'Firebase/Messaging'
  pod 'Firebase/Analytics'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/Auth'
  
  #PagerView
  pod 'FSPagerView'
  
  #SnapKit
  pod 'SnapKit'
  
  #Tabbar
  pod "ESTabBarController-swift"
  
  #Calendar
  pod 'JTAppleCalendar'
  

  target 'JoyfulChurchTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'JoyfulChurchUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
